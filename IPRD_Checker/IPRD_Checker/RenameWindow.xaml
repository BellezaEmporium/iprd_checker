<Window
	x:Class="IPRD_Checker.RenameWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	TextElement.Foreground="{DynamicResource MaterialDesignBody}"
	TextElement.FontWeight="Regular"
	TextElement.FontSize="13"
	TextOptions.TextFormattingMode="Ideal"
	TextOptions.TextRenderingMode="Auto"
	Background="{DynamicResource MaterialDesignPaper}"
	FontFamily="{DynamicResource MaterialDesignFont}"
	ResizeMode="NoResize"
	WindowStartupLocation="CenterScreen"
	Title="Rename radio stream"
	Height="136.532"
	Width="503.963">
	<Grid
		Margin="10">
		<Grid.ColumnDefinitions>
			<ColumnDefinition
				Width="*" />
			<ColumnDefinition
				Width="auto" />
		</Grid.ColumnDefinitions>
		<TextBox
			Name="TxtChannelName"
			VerticalAlignment="Center"
			Margin="0 0 30 0" />
		<Button
			Name="BtnOk"
			Grid.Column="1"
			Width="150"
			Click="Btn_ok_Click">
			<Grid
				Width="150">
				<wpf:PackIcon xmlns:wpf="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf"
					Kind="RenameBox"
					VerticalAlignment="Center" />
				<TextBlock
					HorizontalAlignment="Center"
					Text="Ok"
					FontSize="20" />
			</Grid>
		</Button>
	</Grid>
</Window>
